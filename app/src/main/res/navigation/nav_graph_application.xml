<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_application.xml"
    app:startDestination="@id/fragmentSplash"
    tools:ignore="InvalidNavigation">

    <include app:graph="@navigation/nav_graph_creator" />

    <include app:graph="@navigation/nav_graph_main" />

    <include app:graph="@navigation/nav_graph_video_game_info" />

    <include app:graph="@navigation/nav_graph_platform" />

    <fragment
        android:id="@+id/fragmentSplash"
        android:name="com.madproject.videogames.screen.FragmentSplash"
        android:label="FragmentSplash" />
    <include app:graph="@navigation/nav_graph_store" />

    <fragment
        android:id="@+id/fragmentCompose"
        android:name="com.madproject.videogames.screen.FragmentCompose"
        android:label="FragmentCompose" >
        <deepLink app:uri="jetnavapp://compose" />
    </fragment>

</navigation>